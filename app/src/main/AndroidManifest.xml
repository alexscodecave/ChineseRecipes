<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alexb.chineserecipes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/chineserecipesmobilelogoxxxhdpi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/chineserecipesmobilelogoxxxhdpi"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecipeChoices" />
        <activity android:name=".BreakfastActivity" />
        <activity android:name=".LunchActivity" />
        <activity android:name=".DinnerActivity" />
        <activity android:name=".DessertActivity"></activity>
    </application>

</manifest>